    "gray-matter": "^4.0.3",
    "image-size": "1.2.0",
    "lint": "0.8.19",
    "next": "14.2.11",
    "next-contentlayer2": "^0.5.3",
    "next-themes": "^0.4.4",
    "pliny": "0.4.0",
    "postcss": "^8.4.49",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reading-time": "1.5.0",
